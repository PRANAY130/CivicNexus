rules_version = '2';

service firebase.storage {
  match /b/{bucket}/o {
    // Allow read access to all users
    match /{allPaths=**} {
      allow read;
    }

    // Allow authenticated users to write to the 'tickets' folder
    match /tickets/{ticketId} {
      allow write: if request.auth != null;
    }
  }
}
